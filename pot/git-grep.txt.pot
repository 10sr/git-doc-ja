#. type: Plain text
#: ../../git-grep.txt:7
msgid "git-grep - Print lines matching a pattern"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:26
#, no-wrap
msgid ""
"'git grep' [--cached]\n"
"\t   [-a | --text] [-I] [-i | --ignore-case] [-w | --word-regexp]\n"
"\t   [-v | --invert-match] [-h|-H] [--full-name]\n"
"\t   [-E | --extended-regexp] [-G | --basic-regexp]\n"
"\t   [-F | --fixed-strings] [-n]\n"
"\t   [-l | --files-with-matches] [-L | --files-without-match]\n"
"\t   [-z | --null]\n"
"\t   [-c | --count] [--all-match] [-q | --quiet]\n"
"\t   [--max-depth <depth>]\n"
"\t   [--color | --no-color]\n"
"\t   [-A <post-context>] [-B <pre-context>] [-C <context>]\n"
"\t   [-f <file>] [-e] <pattern>\n"
"\t   [--and|--or|--not|(|)|-e <pattern>...] [<tree>...]\n"
"\t   [--] [<pathspec>...]\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:31
msgid ""
"Look for specified patterns in the tracked files in the work tree, blobs "
"registered in the index file, or blobs in given tree objects."
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:37
#, no-wrap
msgid ""
"--cached::\n"
"\tInstead of searching in the working tree files, check\n"
"\tthe blobs registered in the index file.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:41
#, no-wrap
msgid ""
"-a::\n"
"--text::\n"
"\tProcess binary files as if they were text.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:46
#, no-wrap
msgid ""
"-i::\n"
"--ignore-case::\n"
"\tIgnore case differences between the patterns and the\n"
"\tfiles.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:49
#, no-wrap
msgid ""
"-I::\n"
"\tDon't match the pattern in binary files.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:53
#, no-wrap
msgid ""
"--max-depth <depth>::\n"
"\tFor each <pathspec> given on command line, descend at most <depth>\n"
"\tlevels of directories. A negative value means no limit.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:59
#, no-wrap
msgid ""
"-w::\n"
"--word-regexp::\n"
"\tMatch the pattern only at word boundary (either begin at the\n"
"\tbeginning of a line, or preceded by a non-word character; end at\n"
"\tthe end of a line or followed by a non-word character).\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:63
#, no-wrap
msgid ""
"-v::\n"
"--invert-match::\n"
"\tSelect non-matching lines.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:71
#, no-wrap
msgid ""
"-h::\n"
"-H::\n"
"\tBy default, the command shows the filename for each\n"
"\tmatch.  `-h` option is used to suppress this output.\n"
"\t`-H` is there for completeness and does not do anything\n"
"\texcept it overrides `-h` given earlier on the command\n"
"\tline.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:77
#, no-wrap
msgid ""
"--full-name::\n"
"\tWhen run from a subdirectory, the command usually\n"
"\toutputs paths relative to the current directory.  This\n"
"\toption forces paths to be output relative to the project\n"
"\ttop directory.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:84
#, no-wrap
msgid ""
"-E::\n"
"--extended-regexp::\n"
"-G::\n"
"--basic-regexp::\n"
"\tUse POSIX extended/basic regexp for patterns.  Default\n"
"\tis to use basic regexp.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:89
#, no-wrap
msgid ""
"-F::\n"
"--fixed-strings::\n"
"\tUse fixed strings for patterns (don't interpret pattern\n"
"\tas a regex).\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:92
#, no-wrap
msgid ""
"-n::\n"
"\tPrefix the line number to matching lines.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:102
#, no-wrap
msgid ""
"-l::\n"
"--files-with-matches::\n"
"--name-only::\n"
"-L::\n"
"--files-without-match::\n"
"\tInstead of showing every matched line, show only the\n"
"\tnames of files that contain (or do not contain) matches.\n"
"\tFor better compatibility with 'git diff', --name-only is a\n"
"\tsynonym for --files-with-matches.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:107
#, no-wrap
msgid ""
"-z::\n"
"--null::\n"
"\tOutput \\0 instead of the character that normally follows a\n"
"\tfile name.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:112
#, no-wrap
msgid ""
"-c::\n"
"--count::\n"
"\tInstead of showing every matched line, show the number of\n"
"\tlines that match.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:115
#, no-wrap
msgid ""
"--color::\n"
"\tShow colored matches.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:119
#, no-wrap
msgid ""
"--no-color::\n"
"\tTurn off match highlighting, even when the configuration file\n"
"\tgives the default to color output.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:125
#, no-wrap
msgid ""
"-[ABC] <context>::\n"
"\tShow `context` trailing (`A` -- after), or leading (`B`\n"
"\t-- before), or both (`C` -- context) lines, and place a\n"
"\tline containing `--` between contiguous groups of\n"
"\tmatches.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:128
#, no-wrap
msgid ""
"-<num>::\n"
"\tA shortcut for specifying -C<num>.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:137
#, no-wrap
msgid ""
"-p::\n"
"--show-function::\n"
"\tShow the preceding line that contains the function name of\n"
"\tthe match, unless the matching line is a function name itself.\n"
"\tThe name is determined in the same way as 'git diff' works out\n"
"\tpatch hunk headers (see 'Defining a custom hunk-header' in\n"
"\tlinkgit:gitattributes[5]).\n"
"/ --show-function::\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:140
#, no-wrap
msgid ""
"-f <file>::\n"
"\tRead patterns from <file>, one per line.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:146
#, no-wrap
msgid ""
"-e::\n"
"\tThe next parameter is the pattern. This option has to be\n"
"\tused for patterns starting with - and should be used in\n"
"\tscripts passing user input to grep.  Multiple patterns are\n"
"\tcombined by 'or'.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:155
#, no-wrap
msgid ""
"--and::\n"
"--or::\n"
"--not::\n"
"( ... )::\n"
"\tSpecify how multiple patterns are combined using Boolean\n"
"\texpressions.  `--or` is the default operator.  `--and` has\n"
"\thigher precedence than `--or`.  `-e` has to be used for all\n"
"\tpatterns.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:160
#, no-wrap
msgid ""
"--all-match::\n"
"\tWhen giving multiple pattern expressions combined with `--or`,\n"
"\tthis flag is specified to limit the match to files that\n"
"\thave lines to match all of them.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:165
#, no-wrap
msgid ""
"-q::\n"
"--quiet::\n"
"\tDo not output matched lines; instead, exit with status 0 when\n"
"\tthere is a match and with non-zero status when there isn't.\n"
msgstr ""

#. type: Labeled list
#: ../../git-grep.txt:166
#, no-wrap
msgid "`<tree>...`"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:168
msgid "Search blobs in the trees for specified patterns."
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:172
#, no-wrap
msgid ""
"\\--::\n"
"\tSignals the end of options; the rest of the parameters\n"
"\tare <pathspec> limiters.\n"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:176
#, no-wrap
msgid ""
"<pathspec>...::\n"
"\tIf given, limit the search to paths matching at least one pattern.\n"
"\tBoth leading paths match and glob(7) patterns are supported.\n"
msgstr ""

#. type: Title -
#: ../../git-grep.txt:178
#, no-wrap
msgid "Example"
msgstr ""

#. type: Labeled list
#: ../../git-grep.txt:179
#, no-wrap
msgid "git grep 'time_t' -- '*.[ch]'"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:182
msgid ""
"Looks for `time_t` in all tracked .c and .h files in the working directory "
"and its subdirectories."
msgstr ""

#. type: Labeled list
#: ../../git-grep.txt:183
#, no-wrap
msgid "git grep -e \\'#define\\' --and \\( -e MAX_PATH -e PATH_MAX \\)"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:186
msgid ""
"Looks for a line that has `#define` and either `MAX_PATH` or `PATH_MAX`."
msgstr ""

#. type: Labeled list
#: ../../git-grep.txt:187
#, no-wrap
msgid "git grep --all-match -e NODE -e Unexpected"
msgstr ""

#. type: Plain text
#: ../../git-grep.txt:190
msgid ""
"Looks for a line that has `NODE` or `Unexpected` in files that have lines "
"that match both."
msgstr ""

