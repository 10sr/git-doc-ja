#: ../../cmds-mainporcelain.txt:39
msgid "Show the most recent tag that is reachable from a commit."
msgstr "指定コミットから到達可能な直近のタグを表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:40
#, no-wrap
msgid "linkgit:git-diff[1]"
msgstr "linkgit:git-diff[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:42
msgid "Show changes between commits, commit and working tree, etc."
msgstr "コミット間またはコミットと作業ツリー間などの差分を表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:43
#, no-wrap
msgid "linkgit:git-fetch[1]"
msgstr "linkgit:git-fetch[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:45
msgid "Download objects and refs from another repository."
msgstr "他のリポジトリからオブジェクトと参照(ref)をダウンロードします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:46
#, no-wrap
msgid "linkgit:git-format-patch[1]"
msgstr "linkgit:git-format-patch[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:48
msgid "Prepare patches for e-mail submission."
msgstr "e-mail送信するためのパッチを準備します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:49
#, no-wrap
msgid "linkgit:git-gc[1]"
msgstr "linkgit:git-gc[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:51
msgid "Cleanup unnecessary files and optimize the local repository."
msgstr "不要なファイルを削除し、ローカルリポジトリを最適化します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:52
#, no-wrap
msgid "linkgit:git-grep[1]"
msgstr "linkgit:git-grep[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:54
msgid "Print lines matching a pattern."
msgstr "指定パターンにマッチする行を表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:55
#, no-wrap
msgid "linkgit:git-gui[1]"
msgstr "linkgit:git-gui[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:57
msgid "A portable graphical interface to Git."
msgstr "Gitのポータブルなグラフィカルインターフェース。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:58
#, no-wrap
msgid "linkgit:git-init[1]"
msgstr "linkgit:git-init[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:60
msgid "Create an empty Git repository or reinitialize an existing one."
msgstr "空のGitリポジトリを生成するか、既存のリポジトリを再初期化します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:61
#, no-wrap
msgid "linkgit:git-log[1]"
msgstr "linkgit:git-log[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:63
msgid "Show commit logs."
msgstr "コミットのログを表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:64
#, no-wrap
msgid "linkgit:git-merge[1]"
msgstr "linkgit:git-merge[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:66
msgid "Join two or more development histories together."
msgstr "2つ以上の開発履歴を１つにマージします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:67
#, no-wrap
msgid "linkgit:git-mv[1]"
msgstr "linkgit:git-mv[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:69
msgid "Move or rename a file, a directory, or a symlink."
msgstr ""
"ファイルまたはディレクトリ、シンボリックリンクを移動またはリネームします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:70
#, no-wrap
msgid "linkgit:git-notes[1]"
msgstr "linkgit:git-notes[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:72
msgid "Add or inspect object notes."
msgstr "オブジェクトに注釈を追加または確認します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:73
#, no-wrap
msgid "linkgit:git-pull[1]"
msgstr "linkgit:git-pull[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:75
msgid "Fetch from and integrate with another repository or a local branch."
msgstr "他のリポジトリまたはローカルブランチの変更を取得し、マージします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:76
#, no-wrap
msgid "linkgit:git-push[1]"
msgstr "linkgit:git-push[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:78
msgid "Update remote refs along with associated objects."
msgstr "関連するオブジェクトとともにリモートの参照(ref)を更新します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:79
#, no-wrap
msgid "linkgit:git-rebase[1]"
msgstr "linkgit:git-rebase[1]"

