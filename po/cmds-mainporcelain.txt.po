#: ../../cmds-mainporcelain.txt:81
msgid "Forward-port local commits to the updated upstream head."
msgstr "ローカルのコミットを更新された上流ブランチに移し替えます。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:82
#, no-wrap
msgid "linkgit:git-reset[1]"
msgstr "linkgit:git-reset[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:84
msgid "Reset current HEAD to the specified state."
msgstr "現在のHEADを特定の状態にリセットします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:85
#, no-wrap
msgid "linkgit:git-revert[1]"
msgstr "linkgit:git-revert[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:87
msgid "Revert some existing commits."
msgstr "存在するいくつかのコミットを取り消します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:88
#, no-wrap
msgid "linkgit:git-rm[1]"
msgstr "linkgit:git-rm[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:90
msgid "Remove files from the working tree and from the index."
msgstr "作業ツリーとindexからファイルを削除します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:91
#, no-wrap
msgid "linkgit:git-shortlog[1]"
msgstr "linkgit:git-shortlog[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:93
msgid "Summarize 'git log' output."
msgstr "'git log'の出力を要約します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:94
#, no-wrap
msgid "linkgit:git-show[1]"
msgstr "linkgit:git-show[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:96
msgid "Show various types of objects."
msgstr "様々なタイプのオブジェクトを表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:97
#, no-wrap
msgid "linkgit:git-stash[1]"
msgstr "linkgit:git-stash[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:99
msgid "Stash the changes in a dirty working directory away."
msgstr "作業ディレクトリ内の変更を隠します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:100
#, no-wrap
msgid "linkgit:git-status[1]"
msgstr "linkgit:git-status[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:102
msgid "Show the working tree status."
msgstr "作業ツリーのステータスを表示します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:103
#, no-wrap
msgid "linkgit:git-submodule[1]"
msgstr "linkgit:git-submodule[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:105
msgid "Initialize, update or inspect submodules."
msgstr "サブもジュールを初期化または更新、点検します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:106
#, no-wrap
msgid "linkgit:git-tag[1]"
msgstr "linkgit:git-tag[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:108
msgid "Create, list, delete or verify a tag object signed with GPG."
msgstr ""
"GPGで署名されたタグオブジェクトの生成/削除/検証および一覧表示をします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:109
#, no-wrap
msgid "linkgit:gitk[1]"
msgstr "linkgit:gitk[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:111
msgid "The Git repository browser."
msgstr "Gitリポジトリの閲覧ツール。"
