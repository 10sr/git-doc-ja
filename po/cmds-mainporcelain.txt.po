#: ../../cmds-mainporcelain.txt:15
msgid "List, create, or delete branches."
msgstr "ブランチを一覧表示または作成、削除します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:16
#, no-wrap
msgid "linkgit:git-bundle[1]"
msgstr "linkgit:git-bundle[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:18
msgid "Move objects and refs by archive."
msgstr "オブジェクトと参照(ref)をアーカイブ化して移動させます。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:19
#, no-wrap
msgid "linkgit:git-checkout[1]"
msgstr "linkgit:git-checkout[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:21
msgid "Checkout a branch or paths to the working tree."
msgstr "ブランチまたは特定のパスを作業ツリーにチェックアウトします。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:22
#, no-wrap
msgid "linkgit:git-cherry-pick[1]"
msgstr "linkgit:git-cherry-pick[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:24
msgid "Apply the changes introduced by some existing commits."
msgstr "既存のコミットがしている変更を現在のブランチに適用します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:25
#, no-wrap
msgid "linkgit:git-citool[1]"
msgstr "linkgit:git-citool[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:27
msgid "Graphical alternative to git-commit."
msgstr "git-commitの代替GUIを起動します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:28
#, no-wrap
msgid "linkgit:git-clean[1]"
msgstr "linkgit:git-clean[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:30
msgid "Remove untracked files from the working tree."
msgstr "作業ツリーからGit管理外のファイルを削除します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:31
#, no-wrap
msgid "linkgit:git-clone[1]"
msgstr "linkgit:git-clone[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:33
msgid "Clone a repository into a new directory."
msgstr "リポジトリを複製します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:34
#, no-wrap
msgid "linkgit:git-commit[1]"
msgstr "linkgit:git-commit[1]"

#. type: Plain text
#: ../../cmds-mainporcelain.txt:36
msgid "Record changes to the repository."
msgstr "変更をリポジトリに記録します。"

#. type: Labeled list
#: ../../cmds-mainporcelain.txt:37
#, no-wrap
msgid "linkgit:git-describe[1]"
msgstr "linkgit:git-describe[1]"

